{
  "name": "Chrome controller",
  "version": "1.0",
  "description": "Chrome controller",
  "permissions": ["storage", "tabs", "activeTab", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
